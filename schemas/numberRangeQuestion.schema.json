{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "properties": {
    "type": {
      "type": "string",
      "const": "numberRange"
    },
    "attributes": {
      "type": "object",
      "properties": {
        "label": {
          "type": "string"
        },
        "help": {
          "type": "string"
        },
        "labelTranslationKey": {
          "type": "string"
        }
      },
      "additionalProperties": false
    },
    "meta": {
      "type": "object",
      "properties": {
        "schemaVersion": {
          "default": "1.0",
          "type": "string"
        },
        "title": {
          "type": "string"
        },
        "usageDescription": {
          "type": "string"
        }
      },
      "required": [
        "schemaVersion"
      ],
      "additionalProperties": false
    },
    "columns": {
      "type": "object",
      "properties": {
        "start": {
          "type": "object",
          "properties": {
            "label": {
              "default": "From",
              "type": "string"
            },
            "help": {
              "type": "string"
            },
            "labelTranslationKey": {
              "type": "string"
            },
            "max": {
              "type": "number"
            },
            "min": {
              "default": 0,
              "type": "number"
            },
            "step": {
              "default": 1,
              "type": "number"
            }
          },
          "required": [
            "label",
            "min",
            "step"
          ],
          "additionalProperties": false
        },
        "end": {
          "type": "object",
          "properties": {
            "label": {
              "default": "To",
              "type": "string"
            },
            "help": {
              "type": "string"
            },
            "labelTranslationKey": {
              "type": "string"
            },
            "max": {
              "type": "number"
            },
            "min": {
              "default": 0,
              "type": "number"
            },
            "step": {
              "default": 1,
              "type": "number"
            }
          },
          "required": [
            "label",
            "min",
            "step"
          ],
          "additionalProperties": false
        }
      },
      "required": [
        "start",
        "end"
      ],
      "additionalProperties": false
    },
    "showCommentField": {
      "default": false,
      "type": "boolean"
    }
  },
  "required": [
    "type",
    "attributes",
    "meta",
    "columns",
    "showCommentField"
  ],
  "additionalProperties": false
}